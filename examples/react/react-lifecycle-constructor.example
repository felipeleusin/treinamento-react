class Formulario extends Component {

  constructor(props) {
    super(props)
    this.state = { nome: props.nome }
  }

  render() {
    return (
      <form onSubmit={this.handleSubmit}>
        <h1 ref={(ref) => this.div = ref}>Cabe√ßalho</h1>
        <input type="text" value={this.state.nome}  />
        <button>Enviar!</button>
      </form>
    )
  }
}

